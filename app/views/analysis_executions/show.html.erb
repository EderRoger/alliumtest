<%- model_class = AnalysisExecution -%>
<div class="page-header">
  <h1>Execuções</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Duração:</strong></dt>
  <dd><%= @analysis_execution.duration %></dd>
  <dt><strong>Análise:</strong></dt>
  <dd><%= @analysis_execution.analysis.name %></dd>

</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              analysis_execution_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_analysis_execution_path(@analysis_execution), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              analysis_path(@analysis_execution),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
