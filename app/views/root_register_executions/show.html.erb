<%- model_class = RootRegisterExecution -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:register_execution_id) %>:</strong></dt>
  <dd><%= @root_register_execution.register_execution_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:root_id) %>:</strong></dt>
  <dd><%= @root_register_execution.root_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              root_register_executions_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_root_register_execution_path(@root_register_execution), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              root_register_execution_path(@root_register_execution),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
